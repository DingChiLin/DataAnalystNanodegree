library(ggplot2)
data(diamonds)
ggplot(data = diamonds, aes(price)) +
geom_histogram(binwidth=100) +
ggtitle('Price')
plot1 <- ggplot(data = diamonds, aes(price)) +
geom_histogram(binwidth=100) +
ggtitle('Price')
plot2 <- ggplot(data = diamonds, aes(price)) +
geom_histogram(binwidth=0.01) +
coord_trans(x = 'log10') +
ggtitle('Price (log10)')
grid.arrange(plot1, plot2)
